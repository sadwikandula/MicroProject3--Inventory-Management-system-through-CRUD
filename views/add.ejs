<html lang="en">
    <head>
        <title>Fashion Feet</title>
        <meta charset="utf-8">
        <meta name="viewpoint" content="width=device-width">
        <link rel="stylesheet" href="styles.css">
    </head>

    <body>
        <h4 class="c1">Add New Product</h4>

        <form action="/AddData" method="POST">

            <label>Product Id: </label>
            <input type="number" placeholder="Enter Product ID" name="product_id">
            <br>

            <label>Product Category: </label> &nbsp;
            <input type="text" placeholder="Enter category" name="category">
            <br>

            <label>Type of the Product: </label> &nbsp;
            <input type="text" placeholder="Enter type" name="name">
            <br>

            <label>Size: </label>
            <input type="number" placeholder="Enter size" name="size">
            <br>

            <label>Stock: </label> &nbsp;
            <input type="number" placeholder="Enter Quantity" name="quantity">
            <br>

            <label>Cost Price: </label> &nbsp;
            <input type="number" placeholder="Enter cost" name="cost_price">
            <br>

            <label>Selling Price: </label> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <input type="number" placeholder="Enter sp" name="selling_price">
            <br>

            <button type="submit">submit</button>
        
        </form>

        <a href="/" class="c2">Home Page</a>

    </body>

</html>